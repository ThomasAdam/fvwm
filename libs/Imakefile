#include "../Fvwm.tmpl"

SRCS = \
	ClientMsg.c \
	GetFont.c \
	Grab.c \
	Module.c \
	ModParse.c \
	Parse.c \
	Picture.c \
	Strings.c \
	System.c \
	XResource.c \
	envvar.c \
	safemalloc.c \
	sleep.c \
	wild.c

OBJS = \
	ClientMsg.o \
	GetFont.o \
	Grab.o \
	Module.o \
	ModParse.o \
	Parse.o \
	Picture.o \
	Strings.o \
	System.o \
	XResource.o \
	envvar.o \
	safemalloc.o \
	sleep.o \
	wild.o

NormalLibraryTarget($(LIB), $(OBJS))

#if InstallLibs
InstallLibrary($(LIB),$(FVWMLIBDIR))
#else
install:: lib$(LIB).a
#endif

DependTarget()

