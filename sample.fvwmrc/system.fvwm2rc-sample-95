+ "&Rxvt	standard%mini.term.xpm%" Exec exec rxvt
+ "&Eterm	standard%mini.term.xpm%" Exec exec Eterm
+ "X&iterm	standard%mini.term.xpm%" Exec exec xiterm
+ "&Aterm	standard%mini.term.xpm%" Exec exec aterm
+ "&Gnome Terminal	standard%mini.term.xpm%" Exec exec gnome-terminal 
+ "&Konsole	standard%mini.term.xpm%" Exec exec konsole
+ "&Kvt	standard%mini.term.xpm%" Exec exec kvt
+ "&Dtterm	standard%mini.term.xpm%" Exec exec /usr/bin/dtterm
+ ""                                                Nop
+ "&Xterm	80x40 (7x14 font)%mini.term.xpm%"  Exec exec xterm -n $USER@$HOST -T $USER@$HOST -geometry 80x40 -sb +sk -si -sl 2048 -j -ls -fn 7x14 -fb 7x14bold
+ "Xterm/&root	80x40 (7x14 font)%mini.term.xpm%"  Exec exec xterm -fg red -bg black -n root  -T root  -geometry 80x40 -sb +sk -si -sl 2048 -j -ls -fn 7x14 -fb 7x14bold -e su -l
+ "&NXterm	80x40 (7x14 font)%mini.term.xpm%"  Exec exec nxterm -n $USER -T $USER -geometry 80x40 -sb +sk -si -sl 2048 -j -ls -fn 7x14 -fb 7x14bold
+ "&Rxvt 	80x40 (7x14 font)%mini.term.xpm%"  Exec exec rxvt -n $USER@ -T $USER -g 80x40 -fn 7x14
+ "&Eterm	80x40 (7x14 font)%mini.term.xpm%"  Exec exec Eterm -n $USER -T $USER -g 80x40 --font 7x14
+ "X&iterm	80x40 (7x14 font)%mini.term.xpm%"  Exec exec xiterm -n $USER -T $USER -g 80x40 --font 7x14
+ "&Aterm	80x40 (7x14 font)%mini.term.xpm%"  Exec exec aterm -n $USER -T $USER -g 80x40 --font 7x14
+ ""						Nop
+ "Remote &login	...%mini.connect.xpm%"		FvwmForm FormFvwmRlogin.

