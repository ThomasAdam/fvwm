+ 'Page &4%mini.move1.xpm%'	MoveToPage 1 1
+ '' Nop
+ '&Fvwm%mini.move1.xpm%'	MoveToDesk 0 0
+ '&Devel%mini.move1.xpm%'	MoveToDesk 0 1
+ '&Internet%mini.move1.xpm%'	MoveToDesk 0 2
+ '&Misc%mini.move1.xpm%'	MoveToDesk 0 3

