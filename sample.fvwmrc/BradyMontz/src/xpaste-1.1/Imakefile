       INCLUDES = -I$(TOP) -I$(TOP)/X11
LOCAL_LIBRARIES = $(XAWLIB) $(XMULIB) $(XTOOLLIB) $(XLIB)
           SRCS = xpaste.c Xju.c
           OBJS = xpaste.o Xju.o

