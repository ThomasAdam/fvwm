 Size		50 25
 Position	330 365
 Type		PushButton
 Font		LucidaSans-Bold-12
 Title		{Open}
Main
 Case message of
  SingleClic :
  Begin
   Set $options = {}
   If (GetValue 84) == 1 Then
    Begin
     Set $options = $options {+sb }
    End

   If (GetValue 85) == 1 Then
    Begin
     Set $options = $options {-iconic }
    End

   If (GetTitle 74) == {} Then
    Begin
     Set $remoteCmd = {}
    End
   Else
    Begin
     Set $remoteCmd = {-e krsh } (GetTitle 74)
     If (GetTitle 76) == {} Then
      Begin
      End
     Else
      Begin
       Set $remoteCmd = $remoteCmd { -l } (GetTitle 76)
      End
     If (GetTitle 78) == {} Then
      Begin
      End
     Else
      Begin
       Set $remoteCmd = $remoteCmd { '} (GetTitle 78) {'}
      End
    End

   Set $geometry = (GetTitle 81) {x} (GetTitle 83)

   Do {Exec xterm -fg } $ForeColor { -bg } $BackColor { -fn } $Font { -fb }
$BoldFont { -T "} (GetTitle 72) {" -n "} (GetTitle 72) {" -geometry } $geometry
{ } $options { } $remoteCmd { &}

   Quit
  End
End

