# TerminalSettings.fss
#
# FvwmScript to set up and save terminal (xterm) settings
#
# Eddy J. Gurney
# <eddy@ans.net>
#

WindowTitle	{Terminal Settings}
WindowSize	460 400
WindowPosition	400 400
ForeColor	{Black}
BackColor	{rgb:ae/b2/c3}
ShadowColor	{rgb:92/96/a6}
HilightColor	{White}
# A very small default font, since we want small color push buttons w/ no title
Font		5x7

Init
 Begin
  WarpPointer	3
  Set $CustColor = {rgb:ae/b2/c3}
#If you change these, also change the defaults for Widgets 56/57
  Set $ForeColor = {Black}
  Set $BackColor = {AntiqueWhite}
  Set $ForeSelected = 0
  Set $Font = {6x13}
  Set $BoldFont = {6x13bold}
  Set $options = {}
  Set $remoteCmd = {}
  Set $geometry = {}
 End

Widget 3
Property
 Size 20 20
 Position 20 20
 Type PushButton
 BackColor {White}
Main
 Case message of
  SingleClic :
  Begin
   If $ForeSelected == 1 Then
    Begin
     Set $ForeColor = {White}
    End
   Else
    Begin
     Set $BackColor = {White}
    End
   ChangeForeColor 60 $ForeColor
   ChangeBackColor 60 $BackColor
  End
End

